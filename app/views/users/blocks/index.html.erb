<p id="notice"><%= notice %></p>

<h1>Users Blocks</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users_blocks.each do |users_block| %>
      <tr>
        <td><%= link_to 'Show', users_block %></td>
        <td><%= link_to 'Edit', edit_users_block_path(users_block) %></td>
        <td><%= link_to 'Destroy', users_block, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Users Block', new_users_block_path %>
